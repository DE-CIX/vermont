<?xml version="1.0"?>
<ipfixConfig xmlns="urn:ietf:params:xml:ns:ipfix-config">
  <observationPoint id="1">
    <observationDomainId>4711</observationDomainId>
    <type>pcap</type>
    <parameters>
      <interface>eth1</interface>
      <pcap_filter>ip</pcap_filter>
    </parameters>
    <next>
      <meteringProcessId>1</meteringProcessId>
    </next>
  </observationPoint>
  <meteringProcess id="1">
    <packetSelection>
      <stringFilter>
        <is type="ASCII">XXX blame me</is>
        <idmef>
          <destdir>idmef_work</destdir>
          <template>idmef/templates/datafilter_template.xml</template>
          <sendurl>https://server/idmef/receiver_events.cgi</sendurl>
          <filterid>REGEX-XXX</filterid>
        </idmef>
<!--
                <is not type="ASCII">the good one</isnot>
-->
      </stringFilter>
    </packetSelection>
    <packetReporting>
      <templateId>9876</templateId>
      <reportedIE>
        <ieName>sourceIPv4Address</ieName>
      </reportedIE>
      <reportedIE>
        <ieName>destinationIPv4Address</ieName>
      </reportedIE>
      <reportedIE>
        <ieName>protocolIdentifier</ieName>
      </reportedIE>
    </packetReporting>
    <next>
      <exportingProcessId>1</exportingProcessId>
    </next>
  </meteringProcess>
  <exportingProcess id="1">
    <ipfixPacketRestrictions>
      <maxPacketSize>2000</maxPacketSize>
      <maxExportDelay unit="msec">500</maxExportDelay>
    </ipfixPacketRestrictions>
    <udpTemplateManagement>
      <templateRefreshTimeout unit="sec">1</templateRefreshTimeout>
      <templateRefreshRate>10</templateRefreshRate>
    </udpTemplateManagement>
    <collector>
      <ipAddressType>4</ipAddressType>
      <ipAddress>127.0.0.1</ipAddress>
      <transportProtocol>17</transportProtocol>
      <port>1500</port>
    </collector>
  </exportingProcess>
  <vermont_main>
    <poll_interval unit="msec">500</poll_interval>
    <log_file>log.stat</log_file>
    <log_interval unit="msec">30000</log_interval>
  </vermont_main>
</ipfixConfig>
