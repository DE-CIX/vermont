		<nonFlowKey>
			<ieName>packetDeltaCount</ieName>
		</nonFlowKey>
		<nonFlowKey>
			<ieName>tcpControlBits</ieName>
		</nonFlowKey>
	</rule>
	<expiration>
		<inactiveTimeout unit="sec">3</inactiveTimeout>
		<activeTimeout unit="sec">8</activeTimeout>
	</expiration>
	<pollInterval unit="msec">100</pollInterval>
	<next>5</next>
	</ipfixAggregator>


	<ipfixQueue id="5">
		<entries>10000</entries>
		<next>9</next>
	</ipfixQueue>

	<ipfixFileWriter id="9">
                <observationDomainId>10</observationDomainId>
                <maximumFilesize>8500000</maximumFilesize>
                <destinationPath>/tmp</destinationPath>      
                <filenamePrefix>ipfix_dump</filenamePrefix>
        </ipfixFileWriter>


	<ipfixPrinter id="8">
		<outputType>line</outputType>
	</ipfixPrinter>

</ipfixConfig>
