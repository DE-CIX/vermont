<ipfixConfig>

	<sensorManager id="99">
		<checkinterval>2</checkinterval>
	</sensorManager>

	<ipfixCollector id="3">
		<listener>
			<ipAddress>192.168.63.50</ipAddress>
			<transportProtocol>UDP</transportProtocol>
			<port>2055</port>
		</listener>
		<udpTemplateLifetime>300</udpTemplateLifetime>
	<next>4</next>
	</ipfixCollector>


	<ipfixQueue id="4">
		<entries>10000</entries>
		<next>6</next>
	</ipfixQueue>

	<ipfixAggregator id="6">
	<rule>
		<templateId>306</templateId>
		<flowKey>
			<ieName>sourceMacAddress</ieName>
		</flowKey>
		<flowKey>
			<ieName>destinationMacAddress</ieName>
		</flowKey>
		<flowKey>
			<ieName>sourceIPv4Address</ieName>
		</flowKey>
		<flowKey>
			<ieName>destinationIPv4Address</ieName>
		</flowKey>
		<flowKey>
			<ieName>protocolIdentifier</ieName>
		</flowKey>
		<flowKey>
			<ieName>sourceTransportPort</ieName>
		</flowKey>
		<flowKey>
			<ieName>destinationTransportPort</ieName>
		</flowKey>
		<nonFlowKey>
			<ieName>flowStartMilliSeconds</ieName>
		</nonFlowKey>
		<nonFlowKey>
			<ieName>flowEndMilliSeconds</ieName>
		</nonFlowKey>
		<nonFlowKey>
			<ieName>octetDeltaCount</ieName>
		</nonFlowKey>
		<nonFlowKey>
			<ieName>packetDeltaCount</ieName>
		</nonFlowKey>
		<nonFlowKey>
			<ieName>tcpControlBits</ieName>
		</nonFlowKey>
	</rule>
	<rule>
		<templateId>1910</templateId>
		<flowKey>
			<ieName>sourceMacAddress</ieName>
		</flowKey>
		<flowKey>
			<ieName>destinationMacAddress</ieName>
		</flowKey>
		<flowKey>
			<ieName>sourceIPv4Address</ieName>
		</flowKey>
		<flowKey>
			<ieName>destinationIPv4Address</ieName>
		</flowKey>
		<flowKey>
			<ieName>protocolIdentifier</ieName>
		</flowKey>
		<flowKey>
			<ieName>sourceTransportPort</ieName>
		</flowKey>
		<flowKey>
			<ieName>destinationTransportPort</ieName>
		</flowKey>
		<nonFlowKey>
			<ieName>flowStartMilliSeconds</ieName>
